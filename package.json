  "bugs": {
    "url": "https://github.com/yuriyzhupikov/MERN/issues"
  },
  "homepage": "https://github.com/yuriyzhupikov/MERN#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.9",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
